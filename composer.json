{
  "name": "bngrc/flight-app",
  "description": "Site de gestion des dons de la BNGRC",
  "require": {
    "mikecao/flight": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "controllers\\": "app/controllers/",
      "services\\": "app/services/",
      "repositories\\": "app/repositories/",
      "views\\" : "app/views"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "require-dev": {}
}
